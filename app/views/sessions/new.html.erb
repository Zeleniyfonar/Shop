<h1>Вход</h1>

<div>
  <%= form_for(:session_user, url: sessions_path) do |f| %>
    <%= f.label :email %>
    <%= f.text_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit "Sign in" %>
  <% end %>
  <p>New user? <%= link_to "Sign up now!", registration_path %></p>
</div>